{%extends "layout.html.twig"%}

{% block page_title 'Nos Partenaire' %}    

{% block content %}
<h2>Partners</h2>
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Titre</th>
            <th>Contenu</th>            
            <th>Image</th>
            <th width="200px"></th>
        </tr>
        {% for partner in partners %}
            <tr>
                <td>{{ partner.id }}</td>
                <td>{{ partner.title }}</td>
                <td>{{ partner.content }}</td>
                <td><img src="{{ asset('img/' ~ partner.img) }}"></td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4" align="center">Aucun RÃ©sultat</td>
            </tr>
        {% endfor %}
    </table>

{% endblock %}

{% block stylesheets %}
    {{ parent() }}


{% endblock %}